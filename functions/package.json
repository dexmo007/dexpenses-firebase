{
  "name": "dexpenses-functions",
  "scripts": {
    "lint": "tslint --force --project tsconfig.json",
    "lint:sonar": "npm run lint -- -t json -o .sonar/tslint-report.json",
    "build": "tsc",
    "serve": "npm run build && firebase serve --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test": "mocha -r ts-node/register --require chai-autoload-plugins --recursive \"src/**/*.spec.ts\"",
    "test:single": "mocha -r ts-node/register --require chai-autoload-plugins"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@google-cloud/storage": "^2.4.2",
    "@google-cloud/vision": "^0.25.0",
    "firebase-admin": "~7.0.0",
    "firebase-functions": "^2.2.0",
    "luxon": "^1.11.4"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-datetime": "0.0.32",
    "@types/luxon": "^1.11.1",
    "@types/mocha": "^5.2.6",
    "chai": "^4.2.0",
    "chai-autoload-plugins": "^0.1.0",
    "chai-datetime": "^1.5.0",
    "mocha": "^6.0.1",
    "sonarqube-scanner": "^2.4.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.2.2"
  },
  "private": true
}
