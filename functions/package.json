{
  "name": "dexpenses-functions",
  "scripts": {
    "lint": "tslint --force --project tsconfig.json",
    "lint:sonar": "npm run lint -- -t json -o .sonar/tslint-report.json",
    "build": "rimraf lib/ && tsc --project tsconfig.build.json",
    "serve": "npm run build && firebase serve --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "test": "jest",
    "test:coverage": "jest --silent --coverage --collect-coverage-from=src/**/* --collect-coverage-from=!**/__snapshots__/**"
  },
  "main": "lib/index.js",
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "@dexpenses/core": "^1.1.0",
    "@dexpenses/rule-conditions": "^1.2.0",
    "@google-cloud/bigquery": "^4.1.2",
    "@google-cloud/pubsub": "^0.29.1",
    "@google-cloud/storage": "^3.0.2",
    "@google-cloud/vision": "^1.1.1",
    "@google/maps": "^0.5.5",
    "algoliasearch": "^3.33.0",
    "core-js": "^3.1.4",
    "firebase-admin": "^8.1.0",
    "firebase-functions": "^3.0.1",
    "luxon": "^1.16.0"
  },
  "devDependencies": {
    "@types/algoliasearch": "^3.30.12",
    "@types/google__maps": "^0.5.5",
    "@types/jest": "^24.0.15",
    "@types/luxon": "^1.15.1",
    "chance": "^1.0.18",
    "firebase-functions-test": "^0.1.6",
    "jest": "^24.8.0",
    "jest-date-mock": "^1.0.7",
    "rimraf": "^2.6.3",
    "source-map-support": "^0.5.12",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.5.2"
  },
  "private": true
}
