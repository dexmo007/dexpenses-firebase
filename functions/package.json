{
  "name": "dexpenses-functions",
  "scripts": {
    "lint": "tslint --force --project tsconfig.json",
    "lint:sonar": "npm run lint -- -t json -o .sonar/tslint-report.json",
    "build": "tsc",
    "serve": "npm run build && firebase serve --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "test": "jest",
    "test:cov": "jest --coverage --collect-coverage-from=src/**"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@google-cloud/storage": "^2.4.2",
    "@google-cloud/vision": "^0.25.0",
    "@google/maps": "^0.5.5",
    "firebase-admin": "~7.0.0",
    "firebase-functions": "^2.2.0",
    "luxon": "^1.11.4"
  },
  "devDependencies": {
    "@types/google__maps": "^0.5.5",
    "@types/jest": "^24.0.11",
    "@types/luxon": "^1.11.1",
    "firebase-functions-test": "^0.1.6",
    "jest": "^24.7.1",
    "sonarqube-scanner": "^2.4.0",
    "source-map-support": "^0.5.10",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-microsoft-contrib": "^6.1.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.2.2"
  },
  "private": true
}
