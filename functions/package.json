{
  "name": "dexpenses-functions",
  "scripts": {
    "lint": "tslint --force --project tsconfig.json",
    "lint:sonar": "npm run lint -- -t json -o .sonar/tslint-report.json",
    "build": "tsc --project tsconfig.build.json",
    "serve": "npm run build && firebase serve --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "test": "jest",
    "test:cov": "jest --silent --coverage --collect-coverage-from=src/**/* --collect-coverage-from=!**/__snapshots__/**"
  },
  "main": "lib/index.js",
  "engines": {
    "node": "8"
  },
  "dependencies": {
    "@dexpenses/rule-conditions": "^1.0.5",
    "@google-cloud/bigquery": "^3.0.0",
    "@google-cloud/pubsub": "^0.29.0",
    "@google-cloud/storage": "^2.5.0",
    "@google-cloud/vision": "^0.25.0",
    "@google/maps": "^0.5.5",
    "core-js": "^3.0.1",
    "firebase-admin": "^7.3.0",
    "firebase-functions": "^2.3.1",
    "luxon": "^1.13.2"
  },
  "devDependencies": {
    "@types/gapi.client.vision": "^1.0.1",
    "@types/google__maps": "^0.5.5",
    "@types/jest": "^24.0.13",
    "@types/luxon": "^1.13.0",
    "firebase-functions-test": "^0.1.6",
    "firebase-mock": "^2.2.10",
    "jest": "^24.8.0",
    "sonarqube-scanner": "^2.4.0",
    "source-map-support": "^0.5.12",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-microsoft-contrib": "^6.1.1",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.4.5"
  },
  "private": true
}
